model Location {
    id                String       @id @default(uuid())
    name              String
    address           String?
    country           String?
    region            String?
    city              String?
    description       String?
    storeId           String
    isdefaultLocation Boolean?
    user              User[]
    order             Order[]
    product           Product[]
    store             Organization @relation(fields: [storeId], references: [id], onDelete: Cascade)
    createdAt         DateTime     @default(now())
    updatedAt         DateTime     @updatedAt

    @@map("location")
}
